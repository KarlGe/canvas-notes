{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "Assets/*": ["./src/assets/*"],
      "Components/*": ["./src/components/*"],
      "UI/*": ["./src/components/ui/*"],
      "Config/*": ["./src/config/*"],
      "Helpers/*": ["./src/helpers/*"],
      "Hooks/*": ["./src/hooks/*"],
      "Models/*": ["./src/models/*"],
    }
  }
}
