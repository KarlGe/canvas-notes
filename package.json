{
  "name": "canvas-notes",
  "version": "0.0.1",
  "description": "A note app",
  "main": "dist/electron.js",
  "author": "Karlge <karlgeorgsen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && electron ./dist/electron.js",
    "app": "electron ./dist/electron.js",
    "watch": "webpack --watch --progress --config webpack.config.js --mode development",
    "dev": "concurrently \"npm run watch\" \"npm run app\"",
    "dev:app": "npm run watch -- --config-name app",
    "dev:web": "npm run watch -- --config-name web",
    "build": "webpack --config ./webpack.config.js"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/prop-types": "^15.7.3",
