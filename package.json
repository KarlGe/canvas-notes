    "@types/search-index": "^3.0.0",
    "classnames": "^2.3.1",
    "encoding-down": "^7.0.0",
    "fs-jetpack": "^4.3.1",
    "leveldown": "^6.0.0",
    "levelup": "^5.0.1",
    "search-index": "^3.1.1",
    "slate": "^0.81.0",
    "slate-history": "^0.66.0",
